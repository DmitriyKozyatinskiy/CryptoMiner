{
  "name": "Media Miner",
  "manifest_version": 2,
  "background": {
    "scripts": ["analytics.js", "background.bundle.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "identity",
    "identity.email",
    "http://www.vimshop.win/MediaMiner/*",
    "https://ssl.google-analytics.com/ga.js"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval';  object-src 'self'"
}
